let raceNumber = Math.floor(Math.random() * 1000);
console.log(raceNumber);
const regEarly = true;
const runerAge = 17;
if (runerAge > 18 && regEarly){ 
  raceNumber += 1000; console.log(raceNumber);
}
if (regEarly && runerAge > 18){ 
  console.log('Your stari will be at 9:30 am and your number will be equel ' + raceNumber)
  }else if (!regEarly && runerAge > 18){ 
    console.log('Your stari will be at 11:00 am and your number will be equel ' + raceNumber)
  }else if (runerAge < 18 ){ 
    console.log('You young people and your start wil be at 12:30 pm and your number ='+ raceNumber)
  }
